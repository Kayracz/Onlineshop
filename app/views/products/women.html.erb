
      <!-- trytwofrontend -->
<div class="container" style="margin-top:5%; ">
<h4 class="titulo"> Mujer Deportiva </h4>
<hr style="border-top: 1px solid gray;">
<h4 class="titulo"> Filtrar por </h4>

<hr>
    <div class="row">
      <% cache do %>
  <% @women_product_items.each do |product| %>
        <div class="col-md-3">

            <div class="product-grid2">
                <div class="product-image2">
                    <a href="#"><%= cl_image_tag product.photo %></a>
                    <ul class="social">
                        <li><a href="#" data-tip="Quick View"><i class="fa fa-eye"></i></a></li>
                        <li><a href="#" data-tip="Add to Cart"><i class="fa fa-shopping-cart"></i></a></li>
                    </ul>
                </div>
                <div class="product-content">
                    <h3 class="title"><a href="#"><%= link_to product.title, product %></a></h3>
                    <span class="price"><%= number_to_currency_bo product.price %></span>
                </div>
            </div>
        </div>
   <% end %>
   <% end %>
</div>
</div>
</div>
</div>


